---
{
"stream": {
"$id": "jobs",
"source": "knowledgeGraph",
"destination": "pages",
"filter": {
"entityTypes": [
"job"
]
},
"fields": [
"id",
"uid",
"meta",
"description",
"name",
"slug",
"location",
"hiringOrganization",
"datePosted",
"validThrough",
"employmentType",
"workRemote",
"c_noOfOpenings",
"c_experiencejob",
"c_qualification",
"c_jobCity"

],
"localization": {
"locales": ["en"]
}
},
"urlFormat": "{{slug}}"
}
---

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{name}}</title>
    <meta name="description" content=Category page for {{name}}.>
    {{>head}}
    {{>schema-productCategory}}
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:200,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="../src/style.css">
</head>

<body>


    <div class="w-full pb-10">
        <div class="container mx-auto flex flex-wrap">
            <div class="w-full px-4">
                <h3 class="font-bold text-xl mb-5">Job Descriptions</h3>
                <table class="border-collapse w-full text-sm md:text-base">
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Hiring Organization :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{hiringOrganization}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Date Posted :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{datePosted}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Last Date :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{validThrough}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Employment Type :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{employmentType}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Remote Work :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{workRemote}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Salary :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{c_salery}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            No Of Openings :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{c_noOfOpenings}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            City :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{c_jobCity}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Qualification :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{c_qualification}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Experience :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{c_experiencejob}}
                        </td>
                    </tr>
                    <tr>
                        <td class="border-b border-b-gray-500 py-2 text-gray-500 w-1/4">
                            Job Descriptions :-
                        </td>
                        <td class="border-b border-b-gray-500 py-2 pl-4">
                            {{description}}
                        </td>
                    </tr>
                    <tr>
                        <td class="py-2 text-gray-500">
                            Locations :-
                        </td>
                        <td class="py-2 pl-4">
                            {{location}}
                        </td>
                    </tr>
                </table>
            </div>

        </div>
    </div>
   
</body>

</html>